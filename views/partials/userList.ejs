<div class="row bootstrap snippets center" style="padding-top:7%;display: inline-block;">
        <div>
            <div class="comment-wrapper">
                <div class="panel panel-info">
                    <div class="panel-heading">
                    <% if(followerSource == user.followedUsers){ %>
                        Users <%= user.username %> is following:
                    <% }else{ %>
                        Users following <%= user.username %>:
                    <% } %>
                        <hr>
                    </div>
                    <div class="panel-body">
                        

                        <ul class="media-list">

                        <% followerSource.forEach((connectedUser) => { %>
                        
                            <li class="media commentContainer">
                                <a href="/users/<%=connectedUser._id%>" class="pull-left">
                                    <img src="<%=connectedUser.avatar%>" alt="" class="img-circle">
                                </a>
                                <div class="media-body">
                                    <p>
                                        <%= connectedUser.username %> </a>.
                                    </p>
                            </li>
                        
                        <% }); %>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>